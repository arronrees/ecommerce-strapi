{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Active": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
